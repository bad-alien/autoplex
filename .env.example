# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here

# Plex Configuration
PLEX_URL=http://192.168.1.10:32400
PLEX_TOKEN=your_plex_token_here

# Tautulli Configuration
TAUTULLI_URL=http://192.168.1.10:8181
TAUTULLI_API_KEY=your_tautulli_api_key_here

# Plex Monitor Configuration
# Channel ID where the bot will post alerts if Plex goes down
DISCORD_ALERT_CHANNEL_ID=123456789012345678
# The name of the Plex container on the host system (for log retrieval)
PLEX_CONTAINER_NAME=binhex-plexpass
PLEX_POLL_INTERVAL=30
PLEX_ALERT_COOLDOWN=1800

# Optional: MusicBrainz (for metadata enrichment)
# See https://musicbrainz.org/doc/MusicBrainz_API/Rate_Limiting
MUSICBRAINZ_APP_NAME=Autoalex
MUSICBRAINZ_APP_VERSION=0.1.0
MUSICBRAINZ_CONTACT=your_email@example.com